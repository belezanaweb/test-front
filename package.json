{
  "name": "test-front",
  "version": "1.0.1",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/**/*.stories.{js,jsx,ts,tsx}",
      "!<rootDir>/src/dev/**",
      "!<rootDir>/node_modules/",
      "!<rootDir>/.storybook/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-backgrounds": "^5.1.9",
    "@storybook/addon-centered": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.4",
    "@welldone-software/why-did-you-render": "^3.2.1",
    "a11y-checker": "^2.2.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-standard": "^4.0.0",
    "full-icu": "^1.3.0",
    "husky": "^2.4.1",
    "jest-axe": "^3.1.1",
    "jest-dom": "^3.5.0",
    "jest-emotion": "^10.0.14",
    "jest-extended": "^0.11.1",
    "jest-silent-reporter": "^0.1.2",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-perf-devtool": "^3.1.8",
    "react-scripts": "3.0.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@reach/router": "^1.2.1",
    "emotion-theming": "^10.0.14",
    "minireset.css": "^0.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-credit-card-primitives": "^1.5.1",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "build:storybook": "build-storybook",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "clean": "rimraf build coverage node_modules storybook-static || :",
    "coverage": "$npm_execpath run test -- --watchAll=false --all --coverage",
    "fix": "$npm_execpath run lint -- --fix",
    "lint": "eslint src",
    "reset": "$npm_execpath run clean && $npm_execpath install",
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "storybook": "start-storybook -p 6006",
    "test:ci": "cross-env CI=true $npm_execpath run test -- --color --reporters jest-silent-reporter",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true NODE_PATH=src/dev NODE_ICU_DATA=node_modules/full-icu react-scripts test"
  }
}
